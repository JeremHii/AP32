<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="456.0" prefWidth="755.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ap.apjavafx.controller.ControllerCommerciauxDetails">
   <children>
      <ImageView fitHeight="76.0" fitWidth="237.0" layoutX="492.0" layoutY="386.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/mb.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tabContacts" layoutX="14.0" layoutY="151.0" prefHeight="200.0" prefWidth="432.0">
        <columns>
          <TableColumn fx:id="tcPeriode" prefWidth="210.0" text="Mois" />
          <TableColumn fx:id="tcClients" prefWidth="221.0" text="Clients contactés" />
        </columns>
      </TableView>
      <ComboBox fx:id="cbxAnnee" layoutX="14.0" layoutY="106.0" onAction="#cbxAnneeChanged" prefHeight="25.0" prefWidth="200.0" promptText="Année" />
      <ComboBox fx:id="cbxMois" layoutX="246.0" layoutY="106.0" onAction="#cbxMoisChanged" prefHeight="25.0" prefWidth="200.0" promptText="Mois" visibleRowCount="12" />
      <Button layoutX="14.0" layoutY="390.0" mnemonicParsing="false" onAction="#btnRetourClick" prefHeight="39.0" prefWidth="149.0" text="Retour">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
